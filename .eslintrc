{
  "extends": ["mocha/test", "airbnb"],
  "env": {
      "es6": true,
      "node": true
  },
  "rules": {
    no-console: 0,
    no-useless-escape: 0,
    no-path-concat: 0,
    new-cap: 0,
    no-param-reassign: 0,
    no-underscore-dangle: 0,
    consistent-return: 0,
    func-names: 0,
    prefer-template: 0,
    no-else-return: 0,
    object-shorthand: 0,
    no-unused-expressions: 0,
  }
}
